{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:wdio/recommended",
    "plugin:mocha/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "wdio", "mocha"],
  "rules": { "react/prop-types": "off", "no-unused-vars": "warn" }
}
