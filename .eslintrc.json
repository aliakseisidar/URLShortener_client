{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:wdio/recommended",
    "plugin:mocha/recommended"
  ],
  "overrides": [],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "babelOptions": {
      "presets": ["@babel/preset-react"]
    }
  },
  "plugins": ["react", "wdio", "mocha"],
  "rules": {
    "react/prop-types": "off",
    "no-unused-vars": "warn",
    "wdio/no-pause": "warn"
  }
}
